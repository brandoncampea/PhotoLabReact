# Environment variables for the React app
# Create a .env.local file to override these values for local development

# Set to 'true' to use mock API (no backend required)
# Set to 'false' to use real backend API
VITE_USE_MOCK_API=true

# API base URL - Leave empty to use Vite proxy in development
# For production, set to your actual API URL: https://api.yourdomain.com/api
# VITE_API_URL=/api
